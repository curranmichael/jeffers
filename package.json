{
  "name": "src",
  "version": "0.1.0",
  "private": true,
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:web\" \"npm run dev:electron\"",
    "dev:web": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:models": "vitest run models/",
    "test:ingestion": "vitest run services/ingestion/UrlIngestionWorker.test.ts",
    "electron:build": "echo \"Building Electron code...\" && tsc -p electron/tsconfig.json && echo \"Copying migrations...\" && rm -rf dist/migrations && mkdir -p dist && cp -R models/migrations dist/migrations && echo \"Bundling preload script...\" && npx esbuild electron/preload.ts --bundle --platform=node --external:electron --outfile=dist/electron/preload.js --allow-overwrite --sourcemap --minify --format=cjs --target=node20 && echo \"Creating workers directory...\" && mkdir -p dist/workers && echo \"Bundling readability worker...\" && npx esbuild electron/workers/readabilityWorker.ts --bundle --platform=node --outfile=dist/workers/readabilityWorker.js --allow-overwrite --sourcemap --minify --format=cjs --target=node20 --external:jsdom && echo \"Copying jsdom sync worker...\" && cp node_modules/jsdom/lib/jsdom/living/xhr/xhr-sync-worker.js dist/workers/ && echo \"Bundling main process...\" && npx esbuild electron/main.ts --bundle --platform=node --external:electron --external:better-sqlite3 --external:jsdom --external:onnxruntime-node --external:sharp --outfile=dist/electron/main.js --allow-overwrite --sourcemap --minify --format=cjs --target=node20 && echo \"Electron build complete.\"",
    "electron:start": "npm run electron:build && electron .",
    "dev:electron": "npm run electron:build && npm run rebuild:electron && wait-on dist/electron/main.js dist/electron/preload.js dist/workers/readabilityWorker.js && cross-env NEXT_DEV_SERVER_URL=http://localhost:3000 electron .",
    "dev:all": "npm run dev",
    "rebuild": "npx @electron/rebuild -f -w better-sqlite3",
    "postinstall": "npm run rebuild:electron",
    "dev:next": "next dev -p 3000",
    "build:next": "next build",
    "start:next": "next start",
    "lint:next": "next lint",
    "rebuild:electron": "npx tsx utils/cli/rebuildBetterSqlite3.ts",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "cli:reset-embeddings": "npx ts-node utils/cli/resetEmbeddings.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@browserbasehq/sdk": "^2.5.0",
    "@langchain/community": "^0.3.41",
    "@langchain/core": "^0.3.47",
