{
  "name": "src",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:web\" \"npm run dev:electron\"",
    "dev:web": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "electron:build": "tsc -p electron/tsconfig.json && npx esbuild dist/electron/preload.js --bundle --platform=node --external:electron --outfile=dist/electron/preload.js --allow-overwrite",
    "electron:start": "npm run electron:build && electron dist/electron/main.js",
    "dev:electron": "npm run electron:build && wait-on dist/electron/main.js dist/electron/preload.js dist/shared/ipcChannels.js && cross-env NEXT_DEV_SERVER_URL=http://localhost:3000 electron dist/electron/main.js",
    "dev:all": "npm run dev"
  },
  "dependencies": {
    "@mozilla/readability": "^0.6.0",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/themes": "^3.2.1",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^10.0.0",
    "chromadb-client": "^2.2.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "jsdom": "^26.1.0",
    "lucide-react": "^0.488.0",
    "nanoid": "^5.0.7",
    "next": "15.3.0",
    "p-queue": "^8.1.0",
    "puppeteer": "^24.6.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "sonner": "^1.7.4",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/cli": "^4.1.4",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^35.1.5",
    "esbuild": "^0.20.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "typescript": "^5",
    "wait-on": "^8.0.3"
  }
}
