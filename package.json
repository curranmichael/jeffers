    "p-queue": "^8.1.0",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^4.8.69",
    "puppeteer": "^24.6.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-markdown": "^10.1.0",
    "react-rnd": "^10.5.2",
